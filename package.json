{
  "name": "barte-sdk-web",
  "version": "00000",
  "author": {
    "name": "Gabriel Silva",
    "email": "gabriel.silva@barte.com"
  },
  "license": "ISC",
  "scripts": {
    "build:local": "ENV=local node build.js",
    "build:dev": "ENV=dev node build.js",
    "build:sandbox": "ENV=sandbox node build.js",
    "build:hml": "ENV=hml node build.js",
    "build": "ENV=production node build.js",
    "dev": "node build.js && serve dist"
  },
  "devDependencies": {
    "@yuno-payments/sdk-web-types": "^4.1.0",
    "esbuild": "^0.25.5",
    "serve": "^14.2.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "types": "./dist/types/domain/index.d.ts",
      "import": "./dist/esm/domain/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/domain/index.js"
    },
    "./payment": {
      "types": "./dist/types/domain/payment/index.d.ts",
      "import": "./dist/esm/domain/payment/index.js",
      "require": "./dist/cjs/domain/payment/index.cjs",
      "default": "./dist/esm/domain/payment/index.js"
    },
    "./payment/token": {
      "types": "./dist/types/domain/payment/token/index.d.ts",
      "import": "./dist/esm/domain/payment/token/index.js",
      "require": "./dist/cjs/domain/payment/token/index.cjs",
      "default": "./dist/esm/domain/payment/token/index.js"
    },
    "./payment/checkout": {
      "types": "./dist/types/domain/payment/checkout/index.d.ts",
      "import": "./dist/esm/domain/payment/checkout/index.js",
      "require": "./dist/cjs/domain/payment/checkout/index.cjs",
      "default": "./dist/esm/domain/payment/checkout/index.js"
    },
    "./payment/checkout/wallet": {
      "types": "./dist/types/domain/payment/checkout/wallet/index.d.ts",
      "import": "./dist/esm/domain/payment/checkout/wallet/index.js",
      "require": "./dist/cjs/domain/payment/checkout/wallet/index.cjs",
      "default": "./dist/esm/domain/payment/checkout/wallet/index.js"
    },
    "./payment/*": {
      "types": "./dist/types/domain/payment/*.d.ts",
      "import": "./dist/esm/domain/payment/*.js",
      "require": "./dist/cjs/domain/payment/*.cjs",
      "default": "./dist/esm/domain/payment/*.js"
    },
    "./antifraud": {
      "types": "./dist/types/domain/antifraud/index.d.ts",
      "import": "./dist/esm/domain/antifraud/index.js",
      "require": "./dist/cjs/domain/antifraud/index.cjs",
      "default": "./dist/esm/domain/antifraud/index.js"
    },
    "./antifraud/fingerprint": {
      "types": "./dist/types/domain/antifraud/fingerprint/index.d.ts",
      "import": "./dist/esm/domain/antifraud/fingerprint/index.js",
      "require": "./dist/cjs/domain/antifraud/fingerprint/index.cjs",
      "default": "./dist/esm/domain/antifraud/fingerprint/index.js"
    },
    "./antifraud/*": {
      "types": "./dist/types/domain/antifraud/*.d.ts",
      "import": "./dist/esm/domain/antifraud/*.js",
      "require": "./dist/cjs/domain/antifraud/*.cjs",
      "default": "./dist/esm/domain/antifraud/*.js"
    }
  },
  "dependencies": {
    "@yuno-payments/sdk-web": "2.1.0"
  }
}
